{
  "name": "daikozi-vite-boilerplate",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint:js": "eslint \"{src,cypress,test,.storybook}/**/*.{ts,tsx}\"",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint": "npm run lint:js",
    "lint:icones:js:fix": "eslint \"src/components/designSystem/icons/**/*.{ts,tsx}\" --report-unused-disable-directives --fix",
    "check-types": "tsc --project tsconfig.json",
    "prettier": "prettier .",
    "check-format": "npm run prettier -- --check",
    "validate": "npm-run-all --parallel lint check-types check-format",
    "format": "npm run prettier -- --write",
    "test": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.11.16",
    "@reduxjs/toolkit": "^1.9.3",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.10.0",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addons": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-vite": "^7.0.2",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/testing-react": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/ui": "^0.29.8",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-testing-library": "^5.10.2",
    "husky": "^8.0.0",
    "jsdom": "^21.1.1",
    "msw-storybook-addon": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.7",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.2",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.29.8"
  }
}
